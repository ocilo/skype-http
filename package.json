{
  "name": "skype-http",
  "description": "Unofficial Skype API for Node.js via HTTP",
  "version": "0.0.13",
  "license": "MIT",
  "main": "dist/lib/index",
  "types": "dist/lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/ocilo/skype-http.git"
  },
  "scripts": {
    "prepare": "gulp all:tsconfig.json && gulp :tslint.json && gulp dist",
    "prestart": "gulp example:build",
    "start": "node build/example/example/main.js",
    "pretest": "gulp :lint && gulp lib:build",
    "test": "gulp test",
    "test-online": "OCILO_TEST_ONLINE=true npm test",
    "prepublishOnly": "npm run test-online",
    "lint": "gulp :lint"
  },
  "pre-commit": {
    "run": [
      "test"
    ]
  },
  "keywords": [
    "Skype",
    "Skype-Web",
    "Skype-HTTP"
  ],
  "dependencies": {
    "@types/cheerio": "^0.22.5",
    "@types/form-data": "^2.2.1",
    "@types/lodash": "^4.14.85",
    "@types/request": "^2.0.8",
    "@types/tough-cookie": "^2.3.2",
    "assertion-error": "^1.0.2",
    "async-file": "^2.0.2",
    "big-integer": "^1.6.25",
    "bluebird": "^3.5.1",
    "cheerio": "^1.0.0-rc.2",
    "incident": "^3.0.0",
    "js-sha256": "^0.8.0",
    "lodash": "^4.17.4",
    "request": "^2.83.0",
    "tough-cookie": "^2.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.51",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "fs-extra": "^4.0.2",
    "glob": "^7.0.3",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-cli": "^1.4.0",
    "minimist": "^1.2.0",
    "mocha": "^4.0.1",
    "pre-commit": "^1.2.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "turbo-gulp": "^0.15.8",
    "typescript": "^2.6.1"
  }
}
