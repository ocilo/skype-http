{
  "name": "skype-http",
  "description": "Unofficial Skype API for Node.js via HTTP",
  "version": "0.0.5",
  "license": "MIT",
  "main": "./dist/skype-http.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/demurgos/skype-http.git"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:test && npm run build:demo",
    "build:demo": "tsc -p src/demo",
    "build:lib": "tsc -p src/lib",
    "build:test": "tsc -p src/test",
    "clean": "node tools/clean.js",
    "demo": "npm run build:demo && node build/demo/demo/main.js",
    "lint": "tslint \"src/**/*.ts\"",
    "prepare": "typings install",
    "prepublishOnly": "npm run clean && npm test && npm run build:test && node tools/prepublish.js",
    "publishOnly": "npm run prepublishOnly && npm publish",
    "test:offline": "npm run lint && npm run build:test && mocha build/test/test build/test/test/utils --colors",
    "test": "npm run lint && npm run build:test && mocha build/test --recursive --colors"
  },
  "keywords": [
    "Skype",
    "Skype-Web",
    "Skype-HTTP"
  ],
  "dependencies": {
    "big-integer": "^1.5.6",
    "bluebird": "^3.3.5",
    "cheerio": "^0.19.0",
    "es6-promise": "^3.0.2",
    "incident": "^1.0.5",
    "js-sha256": "^0.3.0",
    "lodash": "^4.12.0",
    "request": "2.61",
    "tough-cookie": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "del": "^2.2.0",
    "fs-extra": "^0.30.0",
    "mocha": "^2.4.5",
    "tslint": "^3.9.0",
    "typescript": "^1.8.10"
  }
}
